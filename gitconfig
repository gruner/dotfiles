[user]
	name = Andrew Gruner
	email = andrew@divineflame.com
[core]
	excludesfile = $HOME/.gitignore
	ignorecase = false
[color]
	status = auto
	diff = auto
	branch = auto
[branch "master"]
	merge = master
[alias]
	who = shortlog -s -n --no-merges
	standup = shortlog --since=yesterday
	uncommit = reset --soft HEAD^
[github]
	user = gruner
	token = <.replace github-token>
[push]
	default = current
